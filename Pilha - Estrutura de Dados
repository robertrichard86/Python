pilha = []

pilha.append(1)
pilha.append(2)
pilha.append(3)
pilha.append(4)
pilha.append(5)

print("Pilha Carregada: ", pilha)

def remover_elemento_da_pilha(pilha):
    removido = pilha.pop()
    print("Elemento Removido:", removido)
    print("Pilha Atualizada: ", pilha)

remover_elemento_da_pilha(pilha)
remover_elemento_da_pilha(pilha)
remover_elemento_da_pilha(pilha)
remover_elemento_da_pilha(pilha)
remover_elemento_da_pilha(pilha)
